{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch MindMingle",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/mindmingle.js",
        "env": {
          "NODE_ENV": "development",
          "DATABASE_URL": "your-database-url",
          "JWT_SECRET": "your-secret"
        },
        "restart": true,
        "console": "integratedTerminal"
      }
    ]
  }
  